{
  "expo": {
    "name": "Kristo As…îre Nnwom",
    "slug": "kan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "kan",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4f4f4"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dishplug.kan",
      "icon": "./assets/images/logo.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dishplug.kan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-asset", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bcbd1f44-1eeb-4ab7-87a3-1405af499730"
      }
    },
    "newArchEnabled": true,
    "owner": "dishplug"
  }
}
